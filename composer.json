{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.0.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "blackfire/php-sdk": "^2.3",
        "cboden/ratchet": "^0.4.4",
        "connectholland/cookie-consent-bundle": "^0.11.0",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "easycorp/easyadmin-bundle": "^4.0",
        "hwi/oauth-bundle": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.9",
        "knpuniversity/oauth2-client-bundle": "^2.16",
        "phpoffice/phpspreadsheet": "^1.22",
        "psr/log": "^3.0",
        "stevenmaguire/oauth2-keycloak": "^5.1",
        "stof/doctrine-extensions-bundle": "^1.7",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "6.3.*",
        "symfony/config": "6.3.*",
        "symfony/console": "6.3.*",
        "symfony/dotenv": "6.3.*",
        "symfony/finder": "6.3.*",
        "symfony/flex": "1.19.2",
        "symfony/form": "6.3.*",
        "symfony/framework-bundle": "6.3.*",
        "symfony/google-mailer": "6.3.*",
        "symfony/http-foundation": "6.3.*",
        "symfony/mailer": "6.3.*",
        "symfony/monolog-bundle": "^3.8",
        "symfony/process": "6.3.*",
        "symfony/proxy-manager-bridge": "6.3.*",
        "symfony/runtime": "6.3.*",
        "symfony/security-bundle": "6.3.*",
        "symfony/twig-bundle": "6.3.*",
        "symfony/ux-chartjs": "^2.12",
        "symfony/validator": "6.3.*",
        "symfony/webpack-encore-bundle": "^1.16",
        "symfony/yaml": "6.3.*",
        "symfonycasts/reset-password-bundle": "*",
        "symfonycasts/verify-email-bundle": "^1.10",
        "twig/cssinliner-extra": "^3.5",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.4",
        "twig/twig": "^2.12|^3.0"
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "6.3.*",
        "symfony/css-selector": "6.3.*",
        "symfony/debug-bundle": "6.3.*",
        "symfony/maker-bundle": "^1.43",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "6.3.*",
        "symfony/var-dumper": "6.3.*",
        "symfony/web-profiler-bundle": "6.3.*"
    }
}
